@article{Chirikjian1994a,
abstract = {This paper presents novel and efficient kinematic modeling$\backslash$ntechniques for {\&}ldquo;hyper-redundant{\&}rdquo; robots. This approach is$\backslash$nbased on a {\&}ldquo;backbone curve{\&}rdquo; that captures the robot's$\backslash$nmacroscopic geometric features. The inverse kinematic, or$\backslash$n{\&}ldquo;hyper-redundancy resolution,{\&}rdquo; problem reduces to$\backslash$ndetermining the time varying backbone curve behavior. To efficiently$\backslash$nsolve the inverse kinematics problem, the authors introduce a$\backslash$n{\&}ldquo;modal{\&}rdquo; approach, in which a set of intrinsic backbone curve$\backslash$nshape functions are restricted to a modal form. The singularities of the$\backslash$nmodal approach, modal non-degeneracy conditions, and modal switching are$\backslash$nconsidered. For discretely segmented morphologies, the authors introduce$\backslash$n{\&}ldquo;fitting{\&}rdquo; algorithms that determine the actuator$\backslash$ndisplacements that cause the discrete manipulator to adhere to the$\backslash$nbackbone curve. These techniques are demonstrated with planar and$\backslash$nspatial mechanism examples. They have also been implemented on a 30$\backslash$ndegree-of-freedom robot prototype},
author = {Chirikjian, Gregory S. and Burdick, Joel W.},
doi = {10.1109/70.294209},
file = {:home/bsaund/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Chirikjian, Burdick - 1994 - Modal approach to hyper-redundant manipulator kinematics.pdf:pdf},
issn = {1042296X},
journal = {IEEE Transactions on Robotics and Automation},
number = {3},
pages = {343--354},
title = {{Modal approach to hyper-redundant manipulator kinematics}},
volume = {10},
year = {1994}
}
