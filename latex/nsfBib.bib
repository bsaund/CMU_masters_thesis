file = {:home/bsaund/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Karavas, Tsagarakis, Caldwell - 2012 - Design, modeling and control of a series elastic actuator for an assistive knee exoskeleton.pdf:pdf},
