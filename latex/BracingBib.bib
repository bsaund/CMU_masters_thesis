pages = {242--247},
title = {{Hybrid control of flexible manipulators with multiple contact}},
url = {http://ieeexplore.ieee.org/xpls/abs{\_}all.jsp?arnumber=292153$\backslash$nhttp://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=292153},
